    "def retrieve_data(filein, percentage = True):\n",
    "    d_results = dict()\n",
    "    d_index = dict([('Incorrect',(0,0)), ('Acceptable',(1,1)), ('Medium',(1,2)), ('High',(1,3))])\n",
    "    d_results['CombinedScore'] = [[0, 0, 0, 0],[0, 0, 0, 0]]\n",
    "    d_results['DockQ'] = [[0, 0, 0, 0],[0, 0, 0, 0]]\n",
    "    with open(filein) as f:\n",
    "        for l in f:\n",
    "            s = l.split()\n",
    "            if s[0] == '#CombScore:':\n",
    "                l_sc = d_index[s[2]][0]\n",
    "                d_results['CombinedScore'][d_index[s[2]][0]][d_index[s[2]][1]] += 1\n",
    "            elif s[0] == '#DockQScore:':\n",
    "                d_results['DockQ'][d_index[s[2]][0]][d_index[s[2]][1]] += 1\n",
    "            elif len(s)>1 and s[-2]== '#TotalCases:':\n",
    "                total = eval(s[-1])\n",
    "    if percentage:\n",
    "        for key in ['CombinedScore','DockQ']:\n",
    "            for ii,l1 in enumerate(d_results[key]):\n",
    "                for jj,l2 in enumerate(d_results[key][ii]):\n",
    "                    d_results[key][ii][jj] = (d_results[key][ii][jj]/total)*100.0\n",
    "                \n",
    "    return d_results"
   ]
