    "                    ):\n",
    "    #\n",
    "    # Figure Plotting with Stacked Bar representation\n",
    "    #\n",
    "    l_qualities = ['Incorrect', 'Acceptable', 'Medium', 'High']\n",
    "    d_color = dict(zip(l_qualities,l_colors))\n",
    "    \n",
    "    # Generate a list of x = [cond1_0, cond1_1, cond2_0, cond2_1,...]\n",
    "    # Generate a second list name_x = [[name1, name1, name2, name2,...], \n",
    "    #                                  ['Incorrect', 'Correct', 'Incorrect', 'Correct',...]]    \n",
    "    x = []\n",
    "    l_cat = ['Incorrect', 'Correct']\n",
    "    name_x_category1 = []\n",
    "    name_x_category2 = []\n",
    "    for cond in l_conditions:\n",
    "        for index,cat in enumerate(l_cat):\n",
    "            x.append(f'{cond}_{index}')\n",
    "            name_x_category1.append(f'{d_conditions[cond]}')\n",
    "            name_x_category2.append(f'{cat}')\n",
    "    name_x = [name_x_category1, name_x_category2] # understood as a multi-label categories\n",
    "\n",
    "    df = df[x]\n",
    "    if not fig_input:\n",
    "        fig = go.Figure()\n",
    "    else:\n",
    "        fig = fig_input\n",
    "\n",
    "    for qual in l_qualities:\n",
    "        if not row_col:\n",
    "            fig.add_trace(go.Bar(\n",
    "                name = qual,\n",
    "                x = name_x,\n",
    "                y = df.loc[qual],\n",
    "                marker_color = d_color[qual],\n",
    "                marker_line_color = d_color[qual],\n",
    "                marker_line_width=0,\n",
    "               ),\n",
    "            )\n",
    "        else:\n",
    "            fig.add_trace(go.Bar(\n",
    "                name = qual,\n",
    "                x = name_x,\n",
    "                y = df.loc[qual],\n",
    "                marker_color = d_color[qual],\n",
    "                marker_line_color = d_color[qual],\n",
    "                marker_line_width=0,\n",
    "               ),\n",
    "                row_col[0],\n",
    "                row_col[1],\n",
    "            )            \n",
    "    if not fig_input:\n",
    "        fig.update_layout(\n",
    "            plot_bgcolor = 'rgba(0,0,0,0)',\n",
    "            barmode='relative',\n",
    "            width=width_bar*len(l_conditions),\n",
    "            height=500,\n",
    "            font=dict(\n",
    "                family='Arial',\n",
    "            ),\n",
    "            yaxis=dict(\n",
    "                title='Percentage of Cases',\n",
    "                titlefont_size=18,\n",
    "                tickfont_size=16,\n",
    "                range=[0,100],\n",
    "                showline=True, linewidth=1, linecolor='black', mirror=True, \n",
    "                showgrid=True, gridwidth=1, gridcolor='LightGrey',#griddash='dash',\n",
    "                ticks='outside',\n",
    "                tickson='boundaries',\n",
    "                ticklen=5,\n",
    "            ),\n",
    "            xaxis=dict(\n",
    "                title='Prediction Modes',\n",
    "                titlefont_size=18,\n",
    "                tickfont_size=16,\n",
    "                showline=True, linewidth=1, linecolor='black', mirror=True, \n",
    "                showgrid=False, gridwidth=1, gridcolor='LightGrey',\n",
    "                ticks='outside',\n",
    "                tickson='boundaries',\n",
    "                ticklen=5,\n",
    "            ),\n",
    "        )       \n",
    "    \n",
    "    if add_legend:\n",
    "        fig.update_layout(\n",
    "            legend=dict(\n",
    "            x=0,\n",
    "            y=1.0,\n",
    "            bgcolor='rgba(0,0,0,0)',\n",
    "            font=dict(\n",
    "                family='Arial',\n",
    "                size=14,\n",
    "                color='black',\n",
    "            ),\n",
    "            )\n",
    "        )\n",
    "    elif add_legend == False:\n",
    "        fig.update_layout(showlegend=False)\n",
    "    else:\n",
    "        pass\n",
    "    \n",
    "    return fig\n",
    "\n"
